  "name": "Minimus (Light)",
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.language",
        "variable.other"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Variable start",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Embedded code markers",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "meta.preprocessor"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Support.construct",
      "scope": [
        "support.function.construct",
        "keyword.other.new"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {}
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {}
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {}
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Library Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {}
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {}
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#93a1a1"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#6c71c4"
      }
    }
